{
    "name": "The F Extension",
    "version": "0.1",
    "description": "Adds the word 'fucking' after every article (a, an, the).",
    "manifest_version": 2,
    "icons": {
        "128": "crown.png"
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "crown.png",
        "default_popup": "popup.html"
    }
}